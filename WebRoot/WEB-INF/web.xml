    <servlet-class>servlet.ValidCodeServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>fff</servlet-name>
    <servlet-class>fff</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>TimeBarChart</servlet-name>
    <servlet-class>servlet.TimeBarChart</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>TimeLineChart</servlet-name>
    <servlet-class>servlet.TimeLineChart</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>CategoryBarChart</servlet-name>
    <servlet-class>servlet.CategoryBarChart</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>CategoryPieChart</servlet-name>
    <servlet-class>servlet.CategoryPieChart</servlet-class>
  </servlet>






 
  <servlet-mapping>
    <servlet-name>ValidCodeServlet</servlet-name>
    <url-pattern>/validCodeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>fff</servlet-name>
    <url-pattern>/servlet/fff</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TimeBarChart</servlet-name>
    <url-pattern>/servlet/TimeBarChart</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TimeLineChart</servlet-name>
    <url-pattern>/servlet/TimeLineChart</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
