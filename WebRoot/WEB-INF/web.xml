  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CategoryPieChart</servlet-name>
    <url-pattern>/servlet/CategoryPieChart</url-pattern>
  </servlet-mapping>
	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
